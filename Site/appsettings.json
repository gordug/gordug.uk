{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CodeScrollerOptions": {
    "SourceFilePath": "wwwroot\\SourceFiles",
    "SearchPattern": "*.cs, *.sh, *.html, *.css, *.js, *.bash, *.xml, *.ts, *.sql"
  },
  "PasswordGeneration": {
    "BaseAddress": "https://passgen.azurewebsites.net/"
  }
}
